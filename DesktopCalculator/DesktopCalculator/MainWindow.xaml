<Window x:Class="DesktopCalculator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DesktopCalculator"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="resultBox" HorizontalAlignment="Left" Height="23" Margin="0,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="332" IsEnabled="False"/>
        <Button x:Name="num1Btn" Content="1" HorizontalAlignment="Left" Margin="10,54,0,0" VerticalAlignment="Top" Width="35" Height="35"/>
        <Button x:Name="executeBtn" Content="Execute" HorizontalAlignment="Left" Margin="160,243,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="num1Btn_Copy" Content="1" HorizontalAlignment="Left" Margin="10,54,0,0" VerticalAlignment="Top" Width="35" Height="35" Click="Num1Btn_Click"/>
        <Button x:Name="num0Btn" Content="0" HorizontalAlignment="Left" Margin="64,211,0,0" VerticalAlignment="Top" Width="35" Height="35" Click="Num1Btn_Click"/>
        <Button x:Name="num9Btn" Content="9" HorizontalAlignment="Left" Margin="120,155,0,0" VerticalAlignment="Top" Width="35" Height="35" Click="Num1Btn_Click"/>
        <Button x:Name="num8Btn" Content="8" HorizontalAlignment="Left" Margin="64,155,0,0" VerticalAlignment="Top" Width="35" Height="35" Click="Num1Btn_Click"/>
        <Button x:Name="num7Btn" Content="7" HorizontalAlignment="Left" Margin="10,155,0,0" VerticalAlignment="Top" Width="35" Height="35" Click="Num1Btn_Click"/>
        <Button x:Name="num6Btn" Content="6" HorizontalAlignment="Left" Margin="120,104,0,0" VerticalAlignment="Top" Width="35" Height="35" Click="Num1Btn_Click"/>
        <Button x:Name="num5Btn" Content="5" HorizontalAlignment="Left" Margin="64,104,0,0" VerticalAlignment="Top" Width="35" Height="35" RenderTransformOrigin="1.829,1.714" Click="Num1Btn_Click"/>
        <Button x:Name="num4Btn" Content="4" HorizontalAlignment="Left" Margin="10,104,0,0" VerticalAlignment="Top" Width="35" Height="35" RenderTransformOrigin="0.571,2.143" Click="Num1Btn_Click"/>
        <Button x:Name="num3Btn" Content="3" HorizontalAlignment="Left" Margin="120,54,0,0" VerticalAlignment="Top" Width="35" Height="35" Click="Num1Btn_Click"/>
        <Button x:Name="num2Btn" Content="2" HorizontalAlignment="Left" Margin="64,54,0,0" VerticalAlignment="Top" Width="35" Height="35" RenderTransformOrigin="0.714,2.571" Click="Num1Btn_Click"/>
    </Grid>
</Window>
